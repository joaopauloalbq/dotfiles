configuration {
  display-drun: "";
  display-window: "";
  display-windowcd: "";
  display-ssh: "";
  show-icons: true;
  drun-display-format: "{icon} {name}";
}


* {
  font: "Noto Sans 10";
  background-color: #383838;
  border-color: #252525;
  text-color: #eff0eb;
  width: 680px;
  height: 465px;
  location: 0;
}

inputbar {
  border: 0 0 1px 0;
  children: [prompt,entry];
  padding: 0px 0px 0px 0px;
}

prompt {
  padding: 0px 13px 0px 13px;
  border: 0 1px 0 0;
  vertical-align: 0.5;
}

textbox {
  background-color: #2e343f;
  border: 0 0 1px 0;
  border-color: #282C33;
}

entry {
  padding: 13px;
}

listview {
  cycle: false;
  scrollbar: false;
  columns: 6;
  cycle: false;
  dynamic: true;
  layout: vertical;
  padding:10px 10px 10px 10px;
  margin: -2px 0px 0px 0px;
  spacing: 10;
}

element {
    orientation:                    vertical;
    border-radius:                  3px;
    padding:                        10px 0px 10px 0px;
}

element-text {
    expand:                         true;
    horizontal-align:               0.5;
    vertical-align:                 0.5;
    padding:                        0px 5px;
    margin:                         0px 0px 0px -3px;
}

element-icon {
    size: 48px;
    border: 0px;
}

element selected {
  background-color: #19979d;
}